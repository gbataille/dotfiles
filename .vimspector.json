{
  "configurations": {
    "1_go": {
      "adapter": "delve",
      "filetypes": ["go"],
      "variables": {},
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "mode": "debug"
      }
    },
    "2_go-test": {
      "adapter": "delve",
      "filetypes": ["go"],
      "variables": {},
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "buildFlags": "-tags=integration,acceptance",
        "mode": "test",
        "showLog": true,
        "trace": "log"
      }
    },
    "3_go-single-test": {
      "adapter": "delve",
      "filetypes": ["go"],
      "variables": {},
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "cwd": "${fileDirname}",
        "args": ["-test.run", "${regex}"],
        "buildFlags": "-tags=integration,acceptance",
        "mode": "test",
        "showLog": true,
        "trace": "log"
      }
    },
    "blocksd": {
      "adapter": "delve",
      "filetypes": ["go"],
      "variables": {},
      "configuration": {
        "request": "launch",
        "program": "${cwd}/cmd/tg-blocksd",
        "args": ["start"],
        "mode": "debug",
        "trace": "log"
      }
    },
    "validatord": {
      "adapter": "delve",
      "filetypes": ["go"],
      "variables": {},
      "configuration": {
        "request": "launch",
        "program": "${cwd}/cmd/tg-validatord",
        "args": ["start", "*${params}"],
        "mode": "debug",
        "trace": "log"
      }
    }
  }
}
